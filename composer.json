{
  "name": "fouladgar/laravel-mobile-verification",
  "description": "",
  "keywords": [
    "mobile-verification",
    "mobile-verifier",
    "laravel-mobile-verification",
    "laravel",
    "lumen",
    "mobile"
  ],
  "support": {
    "issues": "https://github.com/mohammad-fouladgar/laravel-mobile-verification/issues",
    "source": "https://github.com/mohammad-fouladgar/laravel-mobile-verification"
  },
  "authors": [
    {
      "name": "Mohammad Fouladgar",
      "email": "fouladgar.dev@gmail.com",
      "role": "Developer"
    }
  ],
  "license": "MIT",
  "require": {
    "php": "^8.0",
    "illuminate/database": "^9.0",
    "illuminate/support": "^9.0",
    "illuminate/notifications": "^9.0.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "orchestra/testbench": "^7.0",
    "mockery/mockery": "^1.4",
    "php-coveralls/php-coveralls": "^2.1"
  },
  "autoload": {
    "psr-4": {
      "Fouladgar\\MobileVerification\\": "src/",
      "Fouladgar\\MobileVerification\\Database\\Factories\\": "database/factories"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Fouladgar\\MobileVerification\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit --testdox --coverage-text",
    "test-coverage": "vendor/bin/php-coveralls -v"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Fouladgar\\MobileVerification\\ServiceProvider"
      ]
    }
  },
  "config": {
    "discard-changes": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
