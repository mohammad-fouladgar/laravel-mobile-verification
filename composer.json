{
  "name": "fouladgar/laravel-mobile-verification",
  "description": "This package provides convenient methods for making token code, sending and verifying mobile phone verification requests.",
  "keywords": [
    "mobile-verification",
    "mobile-verifier",
    "laravel-mobile-verification",
    "laravel",
    "lumen",
    "mobile"
  ],
  "support": {
    "issues": "https://github.com/mohammad-fouladgar/laravel-mobile-verification/issues",
    "source": "https://github.com/mohammad-fouladgar/laravel-mobile-verification"
  },
  "authors": [
    {
      "name": "Mohammad Fouladgar",
      "email": "fouladgar.dev@gmail.com",
      "role": "Developer"
    }
  ],
  "license": "MIT",
  "require": {
    "php": "^8.2",
    "illuminate/database": "^10.0|^11.0|^12.0",
    "illuminate/support": "^10.0|^11.0|^12.0",
    "illuminate/notifications": "^10.0|^11.0|^12.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.5.17|^11.5.3",
    "orchestra/testbench": "^7.0|^8.0|^10.0",
    "mockery/mockery": "^1.4",
    "php-coveralls/php-coveralls": "^2.1"
  },
  "autoload": {
    "psr-4": {
      "Fouladgar\\MobileVerification\\": "src/",
      "Fouladgar\\MobileVerification\\Database\\Factories\\": "database/factories"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Fouladgar\\MobileVerification\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Fouladgar\\MobileVerification\\ServiceProvider"
      ]
    }
  },
  "config": {
    "discard-changes": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
